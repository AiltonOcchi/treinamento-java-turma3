package br.com.treinamento;

public class Conta {
	
	//Atributos
	String agencia;
	int numeroConta;
	double saldo;
	
	// Métodos
	void transferir(){
		
	}
	
	void sacar(double valor){
		saldo = saldo-valor/
	}
	
	void depositar(double valor) {
		saldo = saldo+valor;
		
		System.out.println("Depósito de: R$"+ valor + 
				"efetuado com sucesso!");
	}

}
