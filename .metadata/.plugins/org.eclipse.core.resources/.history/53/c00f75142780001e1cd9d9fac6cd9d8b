package br.com.treinamento;

public class SwitchCase {

	public static void main(String[] args) {
		
		int diaSemana = 4;
		
		String diaSemanaTexto = "";
		
		switch (diaSemana) {
		case 1:
			diaSemanaTexto =  "Segunda-feira";
			break;
			
		case 2:
			diaSemanaTexto =  "Ter√ßa-feira";
			
		default:
			break;
		}

	}

}
